@import '../styles/colors'
.footer
  display: flex;
  align-self: center;
  width: 100%;
  height: 64px;
  background: secondary-background;
  text-align: center;
  color: text-color;

  &__copyright
   align-items: center;
   width: 13%;
   font-size: 12px;
   line-height: 64px;

  &__list
   display: flex;
   align-self: center;
   width: 35%;
   margin: 0;
   list-style: none;

  &__listItem
   margin-right: 1.5%;
   font-size: 14px;

  &__link
   text-decoration: none;
   color: text-color;
