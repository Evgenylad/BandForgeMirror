@import '../styles/colors'
.footer
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  align-self: center;
  width: 100%;
  height: 64px;
  background: secondary-background;
  text-align: center;
  color: primary-text;

  &__copyright
   align-items: center;
   width: 237px;
   margin-right: 52px;
   font-size: 11px;
   line-height: 64px;
   letter-spacing: 0px;
   font-family: Roboto-Regular;

  &__list
   display: flex;
   align-self: center;
   margin: 0;
   list-style: none;

  &__listItem
   margin-right: 27px;
   font-size: 15px;
   font-family: Roboto-Medium;

  &__link
   text-decoration: none;
   color: primary-text;
   letter-spacing: 0px;
   cursor: pointer;
